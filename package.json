{
  "name": "hardhad-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "chain": "npx hardhat node",
    "start": "cd frontend && yarn dev",
    "deploy-local": "npx hardhat --network localhost run scripts/deploy.js",
    "deploy-goerli": "npx hardhat --network goerli run scripts/deploy.js",
    "clean-graph-node": "rm -rf graph-container/data/",
    "run-graph-node": "cd graph-container && docker-compose up",
    "graph-create-local": "graph create --node http://localhost:8020/ token",
    "graph-ship-local": "yarn graph-codegen && yarn graph-build --network localhost && yarn graph-deploy-local",
    "graph-deploy-local": "cd subgraph && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 token && cd ..",
    "graph-codegen": "cd subgraph && yarn codegen && cd ..",
    "graph-build": "cd subgraph && graph build && cd .."
  },
  "author": "",
  "license": "ISC",
